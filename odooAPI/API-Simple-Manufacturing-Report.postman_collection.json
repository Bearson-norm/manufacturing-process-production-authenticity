	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Script untuk generate tanggal secara otomatis",
					"const today = new Date();",
					"const year = today.getFullYear();",
					"const month = String(today.getMonth() + 1).padStart(2, '0');",
					"const day = String(today.getDate()).padStart(2, '0');",
					"const todayDate = `${year}-${month}-${day}`;",
					"",
					"// Set target_date untuk hari ini",
					"if (!pm.variables.get('target_date')) {",
					"    pm.variables.set('target_date', todayDate);",
					"}",
					"",
					"// Set start_date untuk awal bulan ini",
					"if (!pm.variables.get('start_date')) {",
					"    const startOfMonth = `${year}-${month}-01`;",
					"    pm.variables.set('start_date', startOfMonth);",
					"}",
					"",
					"// Set end_date untuk hari ini",
					"if (!pm.variables.get('end_date')) {",
					"    pm.variables.set('end_date', todayDate);",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Test script untuk validasi response",
					"pm.test('Response has success field', function () {",
					"    const jsonData = pm.response.json();",
					"    pm.expect(jsonData).to.have.property('success');",
					"});",
					"",
					"pm.test('Response contains filter and data', function () {",
					"    const jsonData = pm.response.json();",
					"    if (jsonData.success) {",
					"        pm.expect(jsonData).to.have.property('filter');",
					"        pm.expect(jsonData).to.have.property('total_records');",
					"        pm.expect(jsonData).to.have.property('data');",
					"        pm.expect(jsonData.data).to.be.an('array');",
					"    }",
					"});",
					"",
					"pm.test('Data has correct structure', function () {",
					"    const jsonData = pm.response.json();",
					"    if (jsonData.success && jsonData.data.length > 0) {",
					"        const firstItem = jsonData.data[0];",
					"        pm.expect(firstItem).to.have.property('pic_input');",
					"        pm.expect(firstItem).to.have.property('sku');",
					"        pm.expect(firstItem).to.have.property('mo_id');",
					"        pm.expect(firstItem).to.have.property('roll');",
					"        pm.expect(firstItem).to.have.property('first_authenticity_id');",
					"        pm.expect(firstItem).to.have.property('last_authenticity_id');",
					"        pm.expect(firstItem).to.have.property('completed_at');",
					"        pm.expect(firstItem).to.have.property('production_type');",
					"    }",
					"});"
				]
			}
		}
	],
