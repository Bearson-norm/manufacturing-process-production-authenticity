	],
	"variable": [
		{
			"key": "base_url",
			"value": "http://localhost:1234",
			"type": "string",
			"description": "Base URL untuk API server"
		},
		{
			"key": "api_key",
			"value": "your-api-key-here",
			"type": "string",
			"description": "API Key untuk autentikasi"
		},
		{
			"key": "target_date",
			"value": "2024-01-15",
			"type": "string",
			"description": "Tanggal selesai dalam format YYYY-MM-DD. Otomatis di-set ke tanggal hari ini."
		}
	],
	"auth": {
		"type": "apikey",
		"apikey": [
			{
				"key": "value",
				"value": "{{api_key}}",
				"type": "string"
			},
			{
				"key": "key",
				"value": "X-API-Key",
				"type": "string"
			}
		]
	},
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Script untuk generate tanggal hari ini secara otomatis",
					"// Format: YYYY-MM-DD",
					"const today = new Date();",
					"const year = today.getFullYear();",
					"const month = String(today.getMonth() + 1).padStart(2, '0');",
					"const day = String(today.getDate()).padStart(2, '0');",
					"const todayDate = `${year}-${month}-${day}`;",
					"",
					"// Set variable target_date jika belum di-set atau masih default",
					"if (!pm.variables.get('target_date') || pm.variables.get('target_date') === '2024-01-15') {",
					"    pm.variables.set('target_date', todayDate);",
					"}"
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Test script untuk validasi response",
					"pm.test('Status code is 200', function () {",
					"    pm.response.to.have.status(200);",
					"});",
					"",
					"pm.test('Response has success field', function () {",
					"    const jsonData = pm.response.json();",
					"    pm.expect(jsonData).to.have.property('success');",
					"});",
					"",
					"pm.test('Response contains completed_at date', function () {",
					"    const jsonData = pm.response.json();",
					"    if (jsonData.success) {",
					"        pm.expect(jsonData).to.have.property('completed_at');",
					"        pm.expect(jsonData).to.have.property('total_mo');",
					"        pm.expect(jsonData).to.have.property('data');",
					"    }",
					"});"
				]
			}
		}
	]
}
