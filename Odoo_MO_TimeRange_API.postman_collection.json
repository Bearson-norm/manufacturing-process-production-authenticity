	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					"// Debug: Log variables",
					"const odooBaseUrl = pm.collectionVariables.get('odoo_base_url') || pm.variables.get('odoo_base_url');",
					"const odooSessionId = pm.collectionVariables.get('odoo_session_id') || pm.variables.get('odoo_session_id');",
					"",
					"console.log('=== Pre-request Debug ===');",
					"console.log('Odoo Base URL:', odooBaseUrl);",
					"console.log('Odoo Session ID:', odooSessionId ? odooSessionId.substring(0, 20) + '...' : 'NOT SET');",
					"",
					"// Validate required variables",
					"if (!odooBaseUrl) {",
					"    console.error('ERROR: odoo_base_url is not set!');",
					"    throw new Error('odoo_base_url variable is required');",
					"}",
					"",
					"if (!odooSessionId) {",
					"    console.error('ERROR: odoo_session_id is not set!');",
					"    throw new Error('odoo_session_id variable is required. Get it from Odoo cookie after login.');",
					"}",
					"",
					"// Auto-calculate dates if not set",
					"if (!pm.collectionVariables.get('start_date')) {",
					"    const today = new Date();",
					"    const year = today.getFullYear();",
					"    const month = String(today.getMonth() + 1).padStart(2, '0');",
					"    const day = String(today.getDate()).padStart(2, '0');",
					"    pm.collectionVariables.set('start_date', `${year}-${month}-${day} 00:00:00`);",
					"    console.log('Auto-set start_date:', pm.collectionVariables.get('start_date'));",
					"}",
					"",
					"if (!pm.collectionVariables.get('end_date')) {",
