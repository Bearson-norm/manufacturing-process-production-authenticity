							"// Extract MO numbers from response",
							"if (pm.response.code === 200) {",
							"    const jsonData = pm.response.json();",
							"    if (jsonData.result && Array.isArray(jsonData.result)) {",
							"        const moNumbers = jsonData.result.map(mo => mo.name);",
							"        console.log('MO Numbers:', moNumbers);",
							"        ",
							"        // Set as environment variable for use in other requests",
							"        pm.environment.set('mo_numbers', JSON.stringify(moNumbers));",
							"        ",
